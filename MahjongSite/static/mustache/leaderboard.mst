{{#leaderboards}}
	<div class="leaderboard">
		<h2>LEADERBOARD {{ name }}</h2>
		<table>
			<tr>
				<th>PLACE</th>
				<th>NAME</th>
				<th>AVG SCORE</th>
				<th>GAME COUNT</th>
			</tr>
			{{#scores}}
				<tr>
					<td>{{ place }}</td>
					<td><a href="/playerstats/{{ name }}">{{ name }}</a></td>
					<td>{{ score }}</td>
					<td>{{ count }}</td>
				</tr>
			{{/scores}}
		</table>
	</div>
{{/leaderboards}}
